<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appordertour">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppOrderTour">
        <activity
            android:name=".view.navigation.home_fragment.GetAllTourActivity"
            android:exported="false" />
        <activity
            android:name=".view.detail_tour.CommentActivity"
            android:exported="false" />
        <activity
            android:name=".view.admin.chat_admin.ChatWithUserDetailActivity"
            android:exported="false" />
        <activity
            android:name=".view.admin.chat_admin.ChatAdminActivity"
            android:exported="false" />
        <activity
            android:name=".view.admin.MainAdminActivity"
            android:exported="false" />
        <activity
            android:name=".view.MainStartActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.navigation.SeenLocationActivity"
            android:exported="false" />
        <activity
            android:name=".view.navigation.account.DetailMyTourActivity"
            android:exported="false" />
        <activity
            android:name=".view.navigation.account.MyTourActivity"
            android:exported="false" />
        <activity
            android:name=".view.navigation.home_fragment.AllTourWithCategoryActivity"
            android:exported="false" />
        <activity
            android:name=".view.ThankYouActivity"
            android:exported="false" />
        <activity
            android:name=".view.navigation.booking.DetailBookingTourActivity"
            android:exported="false" />
        <activity
            android:name=".view.navigation.account.InformationAccountActivity"
            android:exported="false" />
        <activity
            android:name=".view.navigation.chat.ChatActivity"
            android:exported="false" />
        <activity
            android:name=".view.detail_tour.DetailTourActivity"
            android:exported="false" />
        <activity
            android:name=".view.auth.LoginActivity"
            android:exported="true"
            android:label="@string/title_activity_login" />
        <activity
            android:name=".view.auth.RegisterActivity"
            android:exported="true"
            android:label="@string/title_activity_register" />
        <activity
            android:name=".view.MainActivity"
            android:exported="true" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>